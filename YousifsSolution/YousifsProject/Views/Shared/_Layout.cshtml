<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="~/CSS/Navbar.css">
    <link rel="stylesheet" href="~/CSS/Shared.css">
    @RenderSection("CSS", false)

</head>
<body>
    <div class="My-navbar">
        <div class="header" onclick="location.href='/index'">
            <h1 class="Z">Z</h1><h1>MS CITY</h1>
        </div>
        <div onclick="location.href='/index'" class="divspan">
            <span class="navspan">Show All Houses</span>
        </div>
        <div onclick="location.href='/Build'" class="divspan">
            <span class="navspan">Build a House</span>
        </div>

        @RenderSection("Navbar", false)

       
    </div>

    @RenderSection("header", false)
    @RenderBody()

    <footer id="footer">
        <partial name="_TimeBox" model="DateTime.Now" />
        &copy; Yousif Ali
    </footer>
    <script src="https://kit.fontawesome.com/2381c935ba.js" crossorigin="anonymous"></script>
    @RenderSection("scripts", false)
    <script src="/JS/Navbar.js"></script>
</body>
</html>
